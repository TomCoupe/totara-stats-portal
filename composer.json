{
    "require": {
        "slim/slim": "4.*",
        "slim/psr7": "^1.7",
        "php-di/php-di": "^7.1",
        "jenssegers/blade": "^2",
        "illuminate/view": "^10",
        "vlucas/phpdotenv": "^5.6",
        "illuminate/database": "^10",
        "illuminate/events": "^10",
        "illuminate/support": "^10",
        "illuminate/console": "^10",
        "illuminate/filesystem": "^10",
        "brick/math": "^0.12.3"
    },
    "require-dev": {
        "spatie/ray": "^1.42"
    },
    "autoload": {
        "files": [
            "./app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app",
            "Bootstrap\\": "bootstrap/"
        }
    },
    "scripts": {
        "post_root_package_install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env);\""
        ],
        "migrate": "./artisan migrate --path=database/migrations",
        "rollback": "./artisan migrate:rollback --path=database/migrations",
        "make:migration": "./artisan make:migration --path=database/migrations"
    },
    "files": [
        "bootstrap/helpers.php",
        "app/vite.php"
    ]
}
